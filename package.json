		"@tailwindcss/vite": "^4.1.13",
		"@tanstack/react-query": "^5.89.0",
		"axios": "^1.12.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"ethers": "^6.15.0",
		"lucide-react": "^0.544.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-redux": "^9.2.0",
