{
	"name": "token-portfolio",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "npm run validate && vite build",
		"format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
		"lint": "eslint .",
		"preview": "vite preview",
